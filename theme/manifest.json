{
  "manifest_version": 2,
  "version": "1.1",
  "name": "Hobbit",
  "theme": {
    "images": {
      "additional_backgrounds": [
        "images/0.png",
        "images/1.jpg",
        "images/2.jpg"
      ]
    },
    "properties": {
      "additional_backgrounds_alignment": [
        "center top",
        "right top",
        "right top"
      ],
      "additional_backgrounds_tiling": [
        "no-repeat",
        "repeat-x",
        "repeat"
      ]
    },
    "colors": {
      "toolbar": "rgba(0, 0, 0, 0)",
      "toolbar_text": "rgb(255, 255, 255)",
      "frame": "rgb(255, 255, 255)",
      "tab_background_text": "rgb(0, 0, 0)",
      "toolbar_field": "rgb(32, 33, 36)",
      "toolbar_field_text": "rgb(255, 255, 255)",
      "tab_line": "rgb(66, 28, 21)",
      "popup": "rgb(32, 33, 36)",
      "popup_text": "rgb(255, 255, 255)",
      "tab_selected": "rgb(66, 28, 21)",
      "tab_loading": "rgb(66, 28, 21)"
    }
  }
}